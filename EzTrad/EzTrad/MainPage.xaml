<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="EzTrad.MainPage"
             xmlns:Views="clr-namespace:EzTrad.Views"
             Title="MainPage"
             BackgroundColor="White">
    <FlyoutPage.Flyout>
        <Views:FlyoutMenuPage/>
    </FlyoutPage.Flyout>
    <FlyoutPage.Detail>
        <NavigationPage>
            <x:Arguments>
                <Views:TongQuanPage Title="Tổng quan"/>
            </x:Arguments>
        </NavigationPage>
    </FlyoutPage.Detail>
</FlyoutPage>
